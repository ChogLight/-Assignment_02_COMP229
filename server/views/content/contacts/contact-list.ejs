<div class="contatiner">
    <h1>
        <%= title %>
    </h1>

    <div class="row">
        <div class="col">
            <table class="table">
                <thead>
                  <tr>
                    <th scope="col">#</th>
                    <th scope="col">Name</th>
                    <th scope="col">Phone</th>
                    <th scope="col">Email</th>
                  </tr>
                </thead>
                <tbody>
                  <% for(let count=0; count < contact.lenght; count++ ) {%>
                    <tr>
                        <th scope="row">
                          <%= count +1 %>
                        </th>
                        <td><%= contact[count].contactName %></td>
                        <td><%= contact[count].contactNumber %></td>
                        <td><%= contact[count].email %></td>
                    </tr>
                  <% } %>
                </tbody>
              </table>
        </div>
    </div>
</div>